{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:migrate": "node ../supabase/scripts/migrate.js",
    "db:migrate:status": "node ../supabase/scripts/migrate.js --status",
    "db:migrate:model": "node ../supabase/scripts/migrate.js --model=",
    "db:seed": "node ../supabase/scripts/seed.js",
    "db:seed:reset": "node ../supabase/scripts/seed.js --reset",
    "db:migration:create": "node ../supabase/scripts/create-migration.js",
    "db:setup": "npm run db:migrate && npm run db:seed",
    "supabase:convert": "node ../supabase/scripts/convert-to-supabase-cli.js",
    "supabase:status": "cd .. && supabase migration list",
    "supabase:push": "cd .. && supabase db push",
    "supabase:pull": "cd .. && supabase db pull",
    "supabase:diff": "cd .. && supabase db diff",
    "supabase:new": "cd .. && supabase migration new",
    "supabase:link": "cd .. && supabase link --project-ref eazovcqdbarejrhxvjdm"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@openai/agents": "^0.3.7",
    "@supabase/supabase-js": "^2.89.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "jspdf": "^2.5.2",
    "jwks-rsa": "^3.1.0",
    "marked": "^12.0.2",
    "mammoth": "^1.10.0",
    "pdf-parse": "^1.1.1",
    "puppeteer": "^24.0.0",
    "sanitize-html": "^2.13.0",
    "xlsx": "^0.18.5",
    "nest-winston": "^1.10.2",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "winston": "^3.19.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
